    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Fishing Slide Show</title>
    <link rel="stylesheet" href="css/project.css">
</head>
<body>
    <%- include ('layouts/menu') -%>
    <main>
        <h1>Neat Cars</h1>

        
    

        <div class="slider">
            <div class="img-box">
                <div class="square">
                    <img src="images/bmw.jpg" alt="" class="slider-img" />
